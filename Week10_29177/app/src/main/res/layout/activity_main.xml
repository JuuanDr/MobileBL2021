<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">
    <Button
        android:id="@+id/btnTutor1"
        android:layout_width="250dp"
        android:layout_height="wrap_content"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toTopOf="@id/btnTutor2"
        android:text="Asyntask"/>
    <Button
        android:id="@+id/btnTutor2"
        android:layout_width="250dp"
        android:layout_height="wrap_content"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@id/btnTutor1"
        app:layout_constraintBottom_toTopOf="@id/btnTutor3"
        android:text="Asyntask Loader"/>
    <Button
        android:id="@+id/btnTutor3"
        android:layout_width="250dp"
        android:layout_height="wrap_content"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@id/btnTutor2"
        app:layout_constraintBottom_toTopOf="@id/btnTutor4"
        android:text="Intent Services"/>
    <Button
        android:id="@+id/btnTutor4"
        android:layout_width="250dp"
        android:layout_height="wrap_content"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@id/btnTutor3"
        app:layout_constraintBottom_toTopOf="@+id/btnTutor5"
        android:text="Started Services"/>
    <Button
        android:id="@+id/btnTutor5"
        android:layout_width="250dp"
        android:layout_height="wrap_content"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@id/btnTutor4"
        app:layout_constraintBottom_toBottomOf="parent"
        android:text="Bound Services"/>
</androidx.constraintlayout.widget.ConstraintLayout>